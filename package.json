    "firebase-admin": "^13.4.0",
    "isomorphic-dompurify": "^2.26.0",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.525.0",
    "next": "^15.4.6",
    "next-auth": "^5.0.0-beta.29",
    "papaparse": "^5.5.3",
    "prisma": "^6.10.1",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.58.1",
    "react-icons": "^5.5.0",
    "sonner": "^2.0.6",
