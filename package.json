    "@types/node": "^24.2.0",
    "@types/papaparse": "^5.3.16",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@types/speakeasy": "^2.0.10",
