{
  "name": "campus-websiteapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup-db": "node scripts/setup-database.js",
    "monitor-db": "node scripts/monitor-supabase.js",
    "check-db": "node -e \"const { createClient } = require('@supabase/supabase-js'); require('dotenv').config({ path: '.env.local' }); const supabase = createClient(process.env.NEXT_PUBLIC_SUPABASE_URL, process.env.NEXT_PUBLIC_SUPABASE_ANON_KEY); supabase.from('users').select('count').limit(1).then(({ data, error }) => console.log(error ? '❌ Error:', error.message : '✅ Base de datos funcionando'));\""
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.10.1",
    "@supabase/supabase-js": "^2.50.2",
    "dotenv": "^17.2.0",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "next": "15.3.4",
    "next-auth": "^4.24.11",
    "prisma": "^6.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.58.1",
    "react-icons": "^5.5.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/aria-query": "^5.0.4",
    "@types/firebase": "^2.4.32",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
